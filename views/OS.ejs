   </section>
    <form action="/OS" method="POST" align="center">
      <label for="newItem">ID do Ônibus</label>  
      <input type="text" name="newItem" id="">
    
    
      
      <br /> <br />  <br>
    
      <button type="submit"  class="btn btn-primary" align= "center">+ Adicionar OS</button>
      <br /> <br />
    </form>
    
    <div class="container">
      <input class="form-control" id="myInput" type="text" placeholder="Procure por uma OS">
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col" width=150>Número da OS</th>
            <th scope="col" width=150>Ônibus associado</th>
            <th scope="col" style="width: 35%;">Descrição OS</th>
            <th scope="col">Sistema</th>
            <th scope="col">Responsável</th>
            <th scope="col">Satus</th>
            <th scope="col"></th>
    
    
          </tr>
        </thead>
        <tbody id="myTable">
          <tr>
  
          <%  newListItems.forEach(function(item) { %>
       

            <form method="POST" action="/OSedit"> 
              <tr>
                <th>
               
                    <%=item.IDos + ""%>   
                  
                </th>
                
                <th>
                 
                    <%=item.ID + ""%>   
                   
                </th> 
                <th>
                
                    <%=item.descricao + ""%>   
              
                </th>
                <th>
               
                    <%=item.sistema + ""%>   
             
                </th>
                <th style= "word-wrap: break-word;">
         
                 <%=item.responsavel + ""%>   
              
              </th>
              <th style= "word-wrap: break-word;">
                
                     <%=item.status + ""%>   
                    
                  </th>
                <!--colocar for dentro do for: for(i=0;i<OS.length;i++) {
                  item.OS[i].IDos%;
               }-->
               <th><button type="submit" value ="<%=item._id%>"name="editButton" class="btn btn-success" >Editar</button></th>

                <!--<th><button type="submit" value ="<%=item._id%>"name="deleteButton" class="btn btn-danger">Apagar</button></th> -->
                

              
              </tr>
            </form>
          

           <% }) %>
  
          </tr>
        </tbody>
      </table>
    </div>
    
      <!-- Bootstrap core JavaScript
        ================================================== -->
      <!-- Placed at the end of the document so the pages load faster -->
      <script>
        $(document).ready(function(){
          $("#myInput").on("keyup", function() {
            var value = $(this).val().toLowerCase();
            $("#myTable tr").filter(function() {
              $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
          });
        });
        </script>
